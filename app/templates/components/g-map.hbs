{{g-maps name="my-map" lat=lat lng=lng zoom=zoom markers=mapMarkers polylines=mapPolyLines}}

<div id="container" class="flexChild rowParent">

  <div id="rowChild20481" class="flexChild">

    {{#bs-accordion as |acc|}}
      {{#each originTruckListFake as |truckOriginFake|}}
        {{#acc.item value=truckOriginFake.Name title=truckOriginFake.Name}}
          <label for={{concat 'origin_' truckOriginFake.Name}}>Origin</label>
          {{input type="text" id=(concat 'origin_' truckOriginFake.Name) name=(concat 'origin_' truckOriginFake.Name) placeholder=truckOriginFake.City disabled=true}}

          <label for={{concat 'destination' truckOriginFake.Name}}>Select a destination</label>
          {{input value=(get this (concat 'destination_' truckOriginFake.Name)) action="destWritten" on="key-up"}}
        {{/acc.item}}
      {{/each}}
    {{/bs-accordion}}
  </div>

  <div id="rowChild23096" class="flexChild button-block">
    {{#bs-button type="primary" onClick=(action "pullTruckStartPosition")}}Pull Data From Server{{/bs-button}}
    {{#bs-button type="primary" onClick=(action "generatePath")}}Generate Path using AlgoGen From Server{{/bs-button}}
    {{#bs-button type="primary" onClick=(action "displayMarkers")}}Display Markers{{/bs-button}}
  </div>
  <div id="rowChild10329" class="flexChild">

  </div>
</div>


{{yield}}
